  FormBox[
   StyleBox[
    RowBox[{
     OverscriptBox[
      SubscriptBox["K", "M"], "\[RightVector]"], "=", 
     RowBox[{
      OverscriptBox["M", "\[RightVector]"], "\[Times]", 
      OverscriptBox["n", "\[RightVector]"]}]}],
    Background->RGBColor[1, 1, 0.85]], TraditionalForm]],ExpressionUUID->
  "f11299ed-beb3-41b0-9e08-d2017bf5abc4"],
