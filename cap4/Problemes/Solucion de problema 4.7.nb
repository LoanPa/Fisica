  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["n", "\[RightVector]"], "ext"], "=", 
    OverscriptBox[
     SubscriptBox["e", "r"], "\[RightVector]"]}], TraditionalForm]],
  ExpressionUUID->"3fa6c38b-9212-4484-8bc3-568a4dc857ba"],
