    RowBox[{"C", " ", "r", 
     OverscriptBox[
      SubscriptBox["e", "z"], "\[RightVector]"]}]}], TraditionalForm]],
  ExpressionUUID->"6d135482-4bd6-436b-95cf-849523906def"],
