 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     OverscriptBox["M", "\[RightVector]"], "(", "r", ")"}], "=", 
    RowBox[{
     FractionBox[
      SubscriptBox["M", "0"], "r"], 
     OverscriptBox[
      SubscriptBox["e", "\[CurlyPhi]"], "\[RightVector]"]}]}], 
   TraditionalForm]],ExpressionUUID->"665f2786-a31f-48c8-a652-072fbc4de4fd"],
